fn foo(a:int) {
	if a > 5 {
		return 6;
	}
	else
	{
		println("top: "+str(a));
		foo(a+1);
		println("down: "+str(a));
	}
}

fn main() {
	foo(0);
}

main();

fn fibonacci(valor:int)
{
    if valor < 3 { 
		if valor <= 0 { return 0; } 
		else { return 1; } 
	}
    else { return fibonacci(valor-1) +  fibonacci(valor-2); }
}

x := 10;
result := fibonacci(x);
println("result: "+str(result));