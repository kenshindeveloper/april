fn main() {
    i := 0;

    for i < (100+1) {
        if (i % 2) == 0 {
            println("i: "+str(i)+", es par");
        } 
        else {
            println("i: "+str(i)+", NO es par");
        }

        i += 1;
    }   
}

main();